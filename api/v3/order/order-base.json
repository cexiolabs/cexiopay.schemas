{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.pay-cex.io/api/v3/order/order-base.json",
	"title": "Order base data",
	"properties": {
		"orderId": {
			"$ref": "https://schemas.pay-cex.io/api/v3/order/identifier.json"
		},
		"clientOrderId": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 128,
			"title": "Optional. Client-side order identifier.",
			"description": "It may used to prevent duplicate orders."
		}
	},
	"required": [
		"orderId",
		"clientOrderId"
	]
}
