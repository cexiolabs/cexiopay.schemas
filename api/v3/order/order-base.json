{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.cexpay.io/api/v3/order/order-base.json",
	"title": "Order base data",
	"properties": {
		"orderId": {
			"$ref": "https://schemas.cexpay.io/api/v3/order/identifier.json",
			"title":"Order unique identifier."
		},
		"clientOrderId": {
			"type": [
				"string",
				"null"
			],
			"maxLength": 128,
			"title": "Optional. Client-side order identifier.",
			"description": "It may used to prevent duplicate orders."
		},
		"instrumentKind": {
			"type": "string",
			"const": "EXCHANGE"
		},
		"createdAt": {
			"ref": "https://schemas.cexpay.io/api/v3/primitives.json#/definitions/date",
			"title": "Order creation date"
		}
	},
	"required": [
		"orderId",
		"clientOrderId",
		"instrumentKind",
		"createdAt"
	]
}
